{
  "name": "notes-api",
  "version": "2.2.0",
  "description": "A Node.js starter for Serverless Framework with ES6 and TypeScript support",
  "main": "handler.js",
  "scripts": {
    "test": "serverless-bundle test",
    "checkList": "serverless invoke local --function list --path mocks/list-event.json",
    "checkCreate": "serverless invoke local --function create --path mocks/create-event.json",
    "checkUpdate": "serverless invoke local --function update --path mocks/update-event.json",
    "checkDelete": "serverless invoke local --function delete --path mocks/delete-event.json",
    "checkGet": "serverless invoke local --function get --path mocks/get-event.json",
    "checkBilling": "serverless invoke local --function billing --path mocks/billing-event.json"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.862.0",
    "serverless-bundle": "^3.2.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.3.3"
  },
  "dependencies": {
    "stripe": "^8.138.0",
    "uuid": "^7.0.3"
  }
}
